                            <th class="col-sm-1">
                                <a href="#" ng-click="outMutePositionsSortType = 'holding'; outMuteSortReverse = !outMuteSortReverse">
                                    Hold
                                    <span ng-show="outMutePositionsSortType == 'holding' && !outMuteSortReverse" class="glyphicon glyphicon-arrow-down"></span>
                                    <span ng-show="outMutePositionsSortType == 'holding' && outMuteSortReverse" class="glyphicon glyphicon-arrow-up"></span>
                                </a>
                            </th>
                            <th class="col-sm-2" style="white-space:nowrap">
                                <a href="#" ng-click="outMutePositionsSortType = 'clientId'; outMuteSortReverse = !outMuteSortReverse">
                                    Client ID
                                    <span ng-show="outMutePositionsSortType == 'clientId' && !outMuteSortReverse" class="glyphicon glyphicon-arrow-down"></span>
                                    <span ng-show="outMutePositionsSortType == 'clientId' && outMuteSortReverse" class="glyphicon glyphicon-arrow-up"></span>
