                                    <span ng-show="inMutePositionsSortType == 'clientName' && inMuteSortReverse" class="glyphicon glyphicon-arrow-up"></span>
                                </a>
                            </th>
                        </tr>
                        <tr ng-repeat="position in outMutePositions | orderBy:outMutePositionsSortType:outMuteSortReverse">
                            <td><a href="#" ng-click="OpenAgentDetail(position.agnetId)">{{position.agentInitial}}</a></td>
                            <td>{{position.timer}}</td>
                            <td>{{position.new}}</td>
                            <td>{{position.holding}}</td>
                            <td>{{position.clientId}}</td>
                            <td style="text-align:left">{{position.outOfRotationReason}}</td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <div ng-class="callPageGridClass">
        <div class="row">
            <div ng-class="actionPageGridItemClass" ng-repeat="action in actionGrid">
                <div class="{{action.style}}">
                    <div class="panel-heading panel-heading-action" style="overflow: hidden; white-space: nowrap; text-overflow: ellipsis;">
                        <span class="pull-left">TYPE: <span class="thick-font">{{action.data.actionType}}</span></span>
                    </div>
                    <div class="panel-body panel-body-action" style="overflow:hidden; white-space: nowrap; text-overflow: ellipsis;">
                        Client: <span class="thick-font">{{action.data.clientName}}</span><br />
                        ID: <span class="thick-font">{{action.data.clientID}}</span>
                        <br />
                        <progressbar class="progress" type="{{action.data.progressBarType}}" value="action.data.timeOverdue" max="action.data.progressBarMaxValue" min="action.data.progressBarMinValue">{{action.data.timeOverdue}} Sec</progressbar>
                    </div>
                </div>
            </div>
        </div>
    </div>
