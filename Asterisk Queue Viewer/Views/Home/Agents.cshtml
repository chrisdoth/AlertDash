    </div>

    <div class="col-sm-12 col-md-6">
        <table class="table table-bordered table-striped table-a1-blue">
            <thead>
                <tr>
                    <th>Time</th>
                    <th>Receptionist</th>
                    <th>Event</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="agtEvent in rotationEvents">
                    <td>{{agtEvent.timeStamp}}</td>
                    <td>{{agtEvent.agentName}}</td>
                    <td>{{agtEvent.event}}</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>